<?xml version="1.0" encoding="utf-8"?>
<Patch>

  <!-- Patch AteLavishMeal - no existing nullifying nodes -->
  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/ThoughtDef[defName='AteLavishMeal']</xpath>
    <value>
      <nullifyingTraits>
        <li>Survivalist</li>
      </nullifyingTraits>
      <nullifyingGenes>
        <li MayRequire="celphcs30.SurvivalistTraitandGene">Survivalist</li>
      </nullifyingGenes>
      <nullifyingPrecepts>
        <li MayRequire="Ludeon.RimWorld.Ideology">SurvivalistFood_Indifferent</li>
